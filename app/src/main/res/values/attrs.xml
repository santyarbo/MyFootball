<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="AspectRatioImageView">
        <attr name="ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="ErrorLayout">
        <attr name="type" format="enum">
            <enum name="unknown" value="0" />
            <enum name="network" value="1" />
            <enum name="not_found" value="2" />
            <enum name="no_results" value="3" />
        </attr>
    </declare-styleable>

</resources>